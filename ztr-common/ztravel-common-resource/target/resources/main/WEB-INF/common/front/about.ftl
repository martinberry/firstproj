	<script>
		$(function(){
			var title = decodeURI(decodeURI('${title!}')) ;
			var menuName = decodeURI(decodeURI('${menuName!}')) ;
			switchMenu('${selector!}', title, menuName) ;
		})
		
		function switchMenu(selector, title, menuName){
			var menus = $(".list-unstyled.leftSecondMenu li") ;
			for(var i=0;i<menus.length;i++){
				$(menus[i]).removeClass("active") ;
				if($(menus[i]).find("a").html() == menuName){
					$(menus[i]).addClass("active") ;
				}
			}
			$(".rightContent.editHeadPortrait").html("") ;
			$(".rightContent.editHeadPortrait").append("<div class=\"aboutTitle\">"+title+"</div>") ;
			$(".rightContent.editHeadPortrait").append($("#"+selector).html()) ;
			$('.travel-question').delegate("li","click",function(){
                $(this).addClass('current').siblings().removeClass('current');
                $('.first-level-question').eq($(this).index()).show().siblings('.first-level-question').hide();
            })
		}
	</script>
