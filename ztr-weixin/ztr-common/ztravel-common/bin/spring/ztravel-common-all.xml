<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
	http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
	http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-3.1.xsd
	http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

	<bean class="com.travelzen.framework.spring.web.context.SpringApplicationContextProvider" />

	<import resource="classpath:ibatis/generator.xml"/>
	   
	<bean id="idGeneratorUtil" class="com.ztravel.common.util.IdGeneratorUtil">
		<property name="sequenceGenerator" ref="ztravel_sequence_generator" />
	</bean>

</beans>
